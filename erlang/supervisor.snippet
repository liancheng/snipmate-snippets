-module(${1:`Filename('', 'untitled')`}).

-behaviour(supervisor).

%% API
-export([start_link/0]).

%% Supervisor callbacks
-export([init/1]).

-define(SERVER, ?MODULE).

%% API functions

start_link() ->
    supervisor:start_link({local, ?SERVER}, ?MODULE, []).

%% Supervisor callbacks

init([]) ->
	${2:Child} = {
		'${3:Name}', {'${4:Module}', start_link, []},
		permanent, 2000, worker, [$4]
	}${5}

